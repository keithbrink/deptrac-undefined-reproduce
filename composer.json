{
    "name": "conveyr/conveyr",
    "description": "Conveyr",
    "keywords": [],
    "license": "proprietary",
    "type": "project",
    "require-dev": {
        "qossmic/deptrac-shim": "0.21.1",
        "spatie/laravel-ray": "^1.29"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "8.1"
        },
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}